            </upload:items>
        </upload:UploadSet> -->


        <Label id="label_3" text="MSDS sheet in case of DGR/Hazardous cargo" />
        <RadioButtonGroup id="radio_3" editable="{=${lrf>/submit}==='No'?true:false}"  columns="2" select="radio_check($event,'3')" selectedIndex="{= ${lrf>/check3}==='Yes'?0:1}">
            <RadioButton text="Yes" />
            <RadioButton text="No" />
        </RadioButtonGroup>

<upload:UploadSet id="check_3" visible="{= ${lrf>/check3}==='Yes'?true:false}" mode="None" instantUpload="false" afterItemAdded=".onAfterItemAdded($event,'check_3')" uploadCompleted=".onUploadCompleted($event,'check_3')" uploadUrl="/PAdani_Logistics_Check_List/" items="{
								path: '/PAdani_Logistics_Check_List',								templateShareable: false}">
            <upload:toolbar>
