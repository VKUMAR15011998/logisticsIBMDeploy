            <upload:items>
                <upload:UploadSetItem fileName="{file>check3_fileName}" url="{file>check3_url}" mediaType="{file>check3_mediaType}" visibleEdit="false" openPressed="stopDefault" />
            </upload:items>
        </upload:UploadSet> -->


        <Label id="label_4" text="Transhipment of cargo permissible" />
        <RadioButtonGroup id="radio_4" editable="{=${lrf>/submit}==='No'?true:false}" columns="2" select="radio_check($event,'4')" selectedIndex="{= ${lrf>/check4}==='Yes'?0:1}">
            <RadioButton text="Yes" />
            <RadioButton text="No" />
        </RadioButtonGroup>


        <Label id="label_5" text="Tarpaulin covering required" />
        <RadioButtonGroup id="radio_5" editable="{=${lrf>/submit}==='No'?true:false}" columns="2" select="radio_check($event,'5')" selectedIndex="{= ${lrf>/check5}==='Yes'?0:1}">
            <RadioButton text="Yes" />
            <RadioButton text="No" />
        </RadioButtonGroup>


        <Label id="label_6" text="Cargo can be loaded ON DECK" />
        <RadioButtonGroup id="radio_6" editable="{=${lrf>/submit}==='No'?true:false}" columns="2" select="radio_check($event,'6')" selectedIndex="{= ${lrf>/check6}==='Yes'?0:1}">
            <RadioButton text="Yes" />
            <RadioButton text="No" />
        </RadioButtonGroup>


        <Label text="{i18n>spec_req}" />
        <Input id="spec" type="Text" editable="{=${lrf>/submit}==='No'?true:false}" value="{lrf>/req}"/>
        <Label text="{i18n>over_rema}" />
        <Input id="overall_remark" editable="{=${lrf>/submit}==='No'?true:false}" type="Text" value="{lrf>/remark}" />



        <Label text="Draft Documents" />
        <ComboBox id="draft_po_no"  change="onSelectItem_draft" selectedKey="BL" items="{                                                    path: 'docs_list>/docs_nos'                                                }">
            <core:Item key="{docs_list>Key}" text="{docs_list>Name}" />
        </ComboBox>
        <Input id="other_draft" type="Text" visible="false" liveChange="cEmpty" placeholder="Enter document name"/>

        <Label ></Label>
         <upload:UploadSet id="dcheck_3" mode="None" instantUpload="false" afterItemAdded=".onAfterItemAdded($event,'dcheck_3')" uploadCompleted=".onUploadCompleted($event,'dcheck_3')" uploadUrl="/PAdani_Logistics_Draft/" items="{
								path: '/PAdani_Logistics_Draft',								templateShareable: false}">
            <upload:toolbar>
            </upload:toolbar>
            <upload:items>
